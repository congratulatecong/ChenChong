<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/main_bg"
    android:orientation="vertical">

    <include layout="@layout/layout_navigation_bar" />

    <Button
        android:id="@+id/btn_effect_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="20dp"
        android:padding="16dp"
        android:text="All Effect Dialog"
        android:textSize="16sp" />

    <Button
        android:id="@+id/btn_action_sheet_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="20dp"
        android:padding="16dp"
        android:text="Action Sheet Dialog"
        android:textSize="16sp" />

    <Button
        android:id="@+id/btn_popup_window"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="20dp"
        android:padding="16dp"
        android:text="Popup Window"
        android:textSize="16sp" />

</LinearLayout>