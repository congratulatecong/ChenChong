<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="23dp" />
            <solid android:color="@color/common_hollow_btn_bg_n" />
            <stroke android:width="2dp" android:color="@color/common_btn_bg_disabled" />
        </shape>
    </item>
    
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="23dp" />
            <solid android:color="@color/common_solid_btn_bg_p" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <corners android:radius="23dp" />
            <solid android:color="@color/common_solid_btn_bg_n" />
        </shape>
    </item>

</selector>